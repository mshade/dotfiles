#!/bin/bash

# Synaptics touchpad settings
synclient MaxTapTime=0
synclient PalmDetect=1

# display
xrandr --output DP3 --off --output DP2 --off --output DP1 --off --output HDMI3 --off --output HDMI2 --off \
   --output HDMI1 --mode 1680x1050 --pos 1280x0 --rotate normal --output LVDS1 --mode 1280x800 --pos 0x0  \
   --rotate normal --output VGA1 --off

